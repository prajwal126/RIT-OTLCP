<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.prajw.ritcourse.Exam">

    <info.hoang8f.widget.FButton
        android:id="@+id/button7"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Checkout"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/cname"
        android:layout_marginBottom="99dp"
        fbutton:buttonColor="@color/fbutton_color_midnight_blue"
        fbutton:shadowColor="@color/fbutton_color_midnight_blue"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="5dp"/>

    <TextView
        android:id="@+id/cname"
        android:layout_width="229dp"
        android:layout_height="48dp"
        android:layout_marginBottom="45dp"
        android:layout_marginTop="41dp"
        android:textAlignment="viewStart"
        android:textColor="@color/cards_label_textColor"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintLeft_toRightOf="@+id/textView34"
        app:layout_constraintRight_toRightOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="@+id/textView34"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="56dp"
        android:layout_height="43dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="17dp"
        android:text="100"
        android:textAlignment="viewStart"
        android:textColor="@color/cards_label_textColor"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="124dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ritlogo"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="116dp"
        android:layout_height="0dp"
        android:text="Course Name"
        android:textColor="@color/cards_label_textColor"
        android:textSize="16sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView36"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView36" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:text="Fee"
        android:textColor="@color/cards_label_textColor"
        android:textSize="16sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView33"
        android:layout_marginTop="230dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView41" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="226dp"
        android:layout_height="41dp"
        android:text="Payment Gateway"
        android:textAlignment="center"
        android:textColor="@color/cards_label_textColor"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/cname"
        android:layout_marginEnd="66dp"
        app:layout_constraintRight_toRightOf="@+id/imageView9"
        android:layout_marginTop="140dp"
        android:layout_marginBottom="138dp"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />
</android.support.constraint.ConstraintLayout>
